{
  "name": "c-ast-generator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx src/parser/server.js",
    "update": "git fetch && git reset --hard origin/main",
    "generate:cpg": "python script/joern.py --src-root $npm_config_data --out-root ${npm_config_output:-tmp}",
    "generate:template": "tsx src/joern/generateAST.ts $npm_config_data ${npm_config_output:-result}",
    "generate:full": "npm run generate:cpg --data=$npm_config_data && npm run generate:template --data=tmp --output=$npm_config_output",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keonoh00/C-AST-Generator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keonoh00/C-AST-Generator/issues"
  },
  "homepage": "https://github.com/keonoh00/C-AST-Generator#readme",
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/cli-progress": "^3.11.6",
    "@types/express": "^5.0.3",
    "eslint": "^9.27.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "dependencies": {
    "cli-progress": "^3.12.0",
    "express": "^5.1.0",
    "zod": "^3.25.67"
  }
}
